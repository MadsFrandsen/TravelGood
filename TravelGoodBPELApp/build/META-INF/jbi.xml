<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://travelgood.ws" xmlns:ns2="http://enterprise.netbeans.org/bpel/BpelModule1/bpelModule1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>TravelGoodBPELApp</name>
            <description>Represents the Service Assembly of TravelGoodBPELApp</description>
        </identification>
        <service-unit>
            <identification>
                <name>TravelGoodBPELApp-TravelGoodBPEL</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>TravelGoodBPEL.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>TravelGoodBPELApp-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="travelgoodPort" service-name="ns1:travelgoodService"/>
                <provider endpoint-name="itineraryPortTypeRole_myRole" service-name="ns2:PartnerLink1"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
