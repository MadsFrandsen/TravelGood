<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://enterprise.netbeans.org/bpel/BpelModule1/bpelModule1" xmlns:ns2="ws.itineraryhandler" xmlns:ns3="http://niceView/" xmlns:ns4="http://flightMapping.ws" xmlns:ns5="http://LameDuck/" xmlns:ns6="http://j2ee.netbeans.org/wsdl/HotelMappingService/java/hotelMapping" xmlns:ns7="http://travelgood.ws" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>TravelGoodApplication</name>
            <description>Represents the Service Assembly of TravelGoodApplication</description>
        </identification>
        <service-unit>
            <identification>
                <name>TravelGoodApplication-TravelGoodBPEL</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>TravelGoodBPEL.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>TravelGoodApplication-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="itineraryHandlerPortTypeRole_partnerRole" service-name="ns1:PartnerLink9"/>
                <provider endpoint-name="itineraryHandlerPortTypeBindingPort" service-name="ns2:itineraryHandlerService"/>
            </connection>
            <connection>
                <consumer endpoint-name="niceViewWebServiceRole_partnerRole" service-name="ns1:PartnerLink6"/>
                <provider endpoint-name="niceViewWebServicePort" service-name="ns3:NiceViewService"/>
            </connection>
            <connection>
                <consumer endpoint-name="flightMappingPortTypeRole_partnerRole" service-name="ns1:PartnerLink4"/>
                <provider endpoint-name="flightMappingPort" service-name="ns4:flightMappingService"/>
            </connection>
            <connection>
                <consumer endpoint-name="LameDuckWebServiceRole_partnerRole" service-name="ns1:PartnerLink2"/>
                <provider endpoint-name="LameDuckWebServicePort" service-name="ns5:LameDuckWebService"/>
            </connection>
            <connection>
                <consumer endpoint-name="hotelMappingPortTypeRole_partnerRole" service-name="ns1:PartnerLink5"/>
                <provider endpoint-name="hotelMappingPort" service-name="ns6:hotelMappingService"/>
            </connection>
            <connection>
                <consumer endpoint-name="itineraryPortTypeBindingPort" service-name="ns7:travelgoodService"/>
                <provider endpoint-name="itineraryPortTypeRole_myRole" service-name="ns1:PartnerLink8"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
